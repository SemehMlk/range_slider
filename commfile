import java.util.*;
import java.util.regex.*;
import java.util.stream.Collectors;

public class CLOExtractor {

    public static Map<String, String> extractCLOs(String text) {
        Map<String, String> cloMap = new HashMap<>();
        Pattern pattern = Pattern.compile("#CLO(.*?)#");
        Matcher matcher = pattern.matcher(text);

        while (matcher.find()) {
            String cloContent = matcher.group(1).trim();
            cloMap.put("#CLO" + cloContent + "#", cloContent);
        }

        return cloMap;
    }

    public static void main(String[] args) {
        String sampleText = "Some text before #CLO12345# and another CLO #CLOTestData# and more #CLO999#";
        Map<String, String> result = extractCLOs(sampleText);
        
        result.forEach((key, value) -> System.out.println("Key: " + key + " -> Value: " + value));
    }
}
