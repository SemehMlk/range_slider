import jakarta.persistence.*;

@Inheritance(strategy = InheritanceType.JOINED)
@DiscriminatorColumn(name = "stream_type")
@Entity
@Table(name = "t_kemm_stream_details")
public abstract class KemmStreamDetails {

    @Id
    @GeneratedValue(strategy = GenerationType.SEQUENCE, generator = "t_kemm_stream_details_gen")
    @SequenceGenerator(name = "t_kemm_stream_details_gen", sequenceName = "t_kemm_stream_details_seq", allocationSize = 1)
    private Long id;

    // Champs communs, s'il y en a
}
