copy-com-folder:
  stage: copy
  image: jfrog/jfrog-cli  # Utiliser l'image Docker officielle avec la CLI JFrog déjà installée
  script:
    - jfrog rt c --url=$JFROG_URL --user=$JFROG_USER --password=$JFROG_PASSWORD my-artifactory-server
    - jfrog rt cp "cacib-kemm-maven-scratch-intranet/com/(*)" "cacib-kemm-maven-stable-intranet/com/{1}/"
