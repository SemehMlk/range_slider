@Mapper(componentModel = "spring")
public interface StreamMapper {

    @Mapping(source = "upStreamQueue", target = "upStreamQueue", ignore = true) // ignore si non applicable
    @Mapping(source = "downStreamQueue", target = "downStreamQueue", ignore = true)
    @Mapping(source = "replyToDownStreamQueue", target = "replyToDownStreamQueue", ignore = true)
    StreamDto toDto(KemmStream stream);

    @Mapping(source = "upStreamQueue", target = "upStreamQueue")
    @Mapping(source = "downStreamQueue", target = "downStreamQueue")
    @Mapping(source = "replyToDownStreamQueue", target = "replyToDownStreamQueue")
    StreamDto toDto(KemmMqStream mqStream);
}
