@Configuration
public class ReferentialImportConfiguration {

    @Bean("migratedImportService")
    public ReferentialImportService<MigratedAccountReferential, MigratedAccountReferentialDto> migratedAccountReferentialImportService(
        ReferentialService<MigratedAccountReferential> referentialService,
        @Qualifier("migratedAccountReferentialMapperImpl") AccountReferentialMapper<MigratedAccountReferential, MigratedAccountReferentialDto> accountReferentialMapper,
        ReferentialImportResultMapper<MigratedAccountReferential, MigratedAccountReferentialDto> referentialImportResultMapper,
        ReferentialFieldValidator<MigratedAccountReferential> referentialFieldValidator) {
        return new ReferentialImportService<>(referentialService, accountReferentialMapper, referentialImportResultMapper, referentialFieldValidator);
    }

    @Bean("nostroImportService")
    public ReferentialImportService<NostroAccountReferential, NostroAccountReferentialDto> nostroAccountReferentialImportService(
        ReferentialService<NostroAccountReferential> referentialService,
        @Qualifier("nostroAccountReferentialMapperImpl") AccountReferentialMapper<NostroAccountReferential, NostroAccountReferentialDto> accountReferentialMapper,
        ReferentialImportResultMapper<NostroAccountReferential, NostroAccountReferentialDto> referentialImportResultMapper,
        ReferentialFieldValidator<NostroAccountReferential> referentialFieldValidator) {
        return new ReferentialImportService<>(referentialService, accountReferentialMapper, referentialImportResultMapper, referentialFieldValidator);
    }
}
