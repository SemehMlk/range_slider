import org.mapstruct.factory.Mappers;

class UserPreferenceMapperTest {

    private UserPreferenceMapper mapper = Mappers.getMapper(UserPreferenceMapper.class);

    @Test
    void testToEntity() {
        UserPreferenceDto dto = new UserPreferenceDto();
        // Configurez votre DTO
        
        UserPreferenceEntity entity = mapper.toEntity(dto);
        
        // Assertions
    }

    @Test
    void testToDto() {
        UserPreferenceEntity entity = new UserPreferenceEntity();
        // Configurez votre entit√©
        
        UserPreferenceDto dto = mapper.toDto(entity);
        
        // Assertions
    }
}
