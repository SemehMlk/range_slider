// transformer-helper.ts

// Définir le type pour les arguments de la fonction transformer
type TransformArgs = string[]; // Un tableau de chaînes de caractères
type TransformResult = string[]; // Résultat attendu : un tableau de chaînes

// Définir le type pour la fonction transformer
export type TransformFunction = (...args: TransformArgs) => TransformResult;

// Créer et exporter le Map
export const transformerMap: Map<string, TransformFunction> = new Map();

// Ajouter la clé 'subscribedApplication' et la fonction transformer
transformerMap.set('subscribedApplication', (...args: string[]): TransformResult => {
  return args;
});
