comparator: (filterLocalDateAtMidnight: Date, cellValue: string) => {
    // Convertir cellValue en objet Date
    const cellDate = new Date(cellValue);

    // Normaliser les deux dates Ã  minuit, en ignorant les heures et fuseaux horaires
    const filterDateAtMidnight = new Date(
        filterLocalDateAtMidnight.getFullYear(),
        filterLocalDateAtMidnight.getMonth(),
        filterLocalDateAtMidnight.getDate()
    );

    const cellDateAtMidnight = new Date(
        cellDate.getFullYear(),
        cellDate.getMonth(),
        cellDate.getDate()
    );

    console.log('Filter Date:', filterDateAtMidnight);
    console.log('Cell Date:', cellDateAtMidnight);

    // Comparaison
    if (cellDateAtMidnight < filterDateAtMidnight) {
        console.log('case1');
        return -1;
    } else if (cellDateAtMidnight > filterDateAtMidnight) {
        console.log('case2');
        return 1;
    } else {
        console.log('case3');
        return 0;
    }
}
