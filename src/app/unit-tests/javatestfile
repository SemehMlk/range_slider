import org.springframework.boot.context.properties.ConfigurationProperties;
import org.springframework.context.annotation.Configuration;

@Configuration
@ConfigurationProperties(prefix = "stream.alias")
public class StreamProperties {

    private String sndFlexTo;
    private String rcvToFlex;

    public String getSndFlexTo() {
        return sndFlexTo;
    }

    public void setSndFlexTo(String sndFlexTo) {
        this.sndFlexTo = sndFlexTo;
    }

    public String getRcvToFlex() {
        return rcvToFlex;
    }

    public void setRcvToFlex(String rcvToFlex) {
        this.rcvToFlex = rcvToFlex;
    }
}
