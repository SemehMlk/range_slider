@Configuration
public class ReferentialImportConfiguration {

    @Bean("migratedImportResultMapper")
    public ReferentialImportResultMapper<MigratedAccountReferential, MigratedAccountReferentialDto> migratedImportResultMapper(
        @Qualifier("migratedAccountReferentialMapperImpl") AccountReferentialMapper<MigratedAccountReferential, MigratedAccountReferentialDto> accountReferentialMapper) {
        return new ReferentialImportResultMapper<>(accountReferentialMapper);
    }

    @Bean("nostroImportResultMapper")
    public ReferentialImportResultMapper<NostroAccountReferential, NostroAccountReferentialDto> nostroImportResultMapper(
        @Qualifier("nostroAccountReferentialMapperImpl") AccountReferentialMapper<NostroAccountReferential, NostroAccountReferentialDto> accountReferentialMapper) {
        return new ReferentialImportResultMapper<>(accountReferentialMapper);
    }
}
