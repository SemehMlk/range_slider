   default KemmStream toKemmStream(StreamDto dto) {
        if (dto == null) {
            throw new IllegalArgumentException("StreamDto cannot be null");
        }
        if ("MQ".equals(dto.getType())) {
            return toKemmMqStream((MqStreamDto) dto);
        } else {
            throw new IllegalArgumentException("Unknown stream type: " + dto.getType());
        }
    }
