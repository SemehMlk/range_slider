public static MessageParseResponse toMessageParseResponse(ParsedCanonicalBean parsedCanonicalBean) {
    MessageParseResponse messageParseResponse = new MessageParseResponse();

    // Déclaration de canonicalBean juste avant son utilisation
    CanonicalBean canonicalBean = new CanonicalBean();
    canonicalBean.senderBic(parsedCanonicalBean.getSenderBic());
    canonicalBean.setReceiverBic(parsedCanonicalBean.getReceiverBic());
    canonicalBean.setAmount(parsedCanonicalBean.getAmount());
    canonicalBean.setMessageType(parsedCanonicalBean.getMessageType());
    canonicalBean.setCurrency(parsedCanonicalBean.getCurrency());
    canonicalBean.setTrn(parsedCanonicalBean.getTrn());
    canonicalBean.setUetr(parsedCanonicalBean.getUetr());
    canonicalBean.setValueDate(parsedCanonicalBean.getValueDate());
    canonicalBean.setMur(parsedCanonicalBean.getMur());
    canonicalBean.setRoutingCode(parsedCanonicalBean.getRoutingCode());

    messageParseResponse.setCanonicalBean(canonicalBean); // Utilisation immédiate
    return messageParseResponse;
}
