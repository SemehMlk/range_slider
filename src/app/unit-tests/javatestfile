function dateComparator(filterDate, cellValue) {
    if (!cellValue) return -1;

    // Convertir la valeur de la cellule en objet Date
    const cellDate = new Date(cellValue);
    const filterDateAtMidnight = new Date(filterDate.getFullYear(), filterDate.getMonth(), filterDate.getDate());
    const cellDateAtMidnight = new Date(cellDate.getFullYear(), cellDate.getMonth(), cellDate.getDate());

    // Comparaison des dates normalisées à minuit
    if (cellDateAtMidnight < filterDateAtMidnight) {
        return -1;
    } else if (cellDateAtMidnight > filterDateAtMidnight) {
        return 1;
    } else {
        return 0;
    }
}

{
    headerName: "Date Column",
    field: "dateField",
    filter: "agDateColumnFilter",
    filterParams: {
        comparator: dateComparator,  // Utilisez le comparateur personnalisé
        inRangeInclusive: true  // Pour inclure les limites dans le filtre between
    },
    // autres configurations...
}
