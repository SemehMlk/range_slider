@Configuration
public class ReferentialImportConfiguration {

    @Bean("migratedImportResultMapper")
    public ReferentialImportResultMapper<MigratedAccountReferential, MigratedAccountReferentialDto> migratedAccountReferentialResultMapper(
        @Qualifier("migratedAccountReferentialMapperImpl") AccountReferentialMapper<MigratedAccountReferential, MigratedAccountReferentialDto> accountReferentialMapper) {
        return new ReferentialImportResultMapper<>(accountReferentialMapper);
    }

    @Bean("nostroImportResultMapper")
    public ReferentialImportResultMapper<NostroAccountReferential, NostroAccountReferentialDto> nostroAccountReferentialResultMapper(
        @Qualifier("nostroAccountReferentialMapperImpl") AccountReferentialMapper<NostroAccountReferential, NostroAccountReferentialDto> accountReferentialMapper) {
        return new ReferentialImportResultMapper<>(accountReferentialMapper);
    }

    @Bean("migratedImportService")
    public ReferentialImportService<MigratedAccountReferential, MigratedAccountReferentialDto> migratedAccountReferentialImportService(
        ReferentialService<MigratedAccountReferential> referentialService,
        @Qualifier("migratedAccountReferentialMapperImpl") AccountReferentialMapper<MigratedAccountReferential, MigratedAccountReferentialDto> accountReferentialMapper,
        @Qualifier("migratedImportResultMapper") ReferentialImportResultMapper<MigratedAccountReferential, MigratedAccountReferentialDto> referentialImportResultMapper) {
        return new ReferentialImportService<>(referentialService, accountReferentialMapper, referentialImportResultMapper);
    }

    @Bean("nostroImportService")
    public ReferentialImportService<NostroAccountReferential, NostroAccountReferentialDto> nostroAccountReferentialImportService(
        ReferentialService<NostroAccountReferential> referentialService,
        @Qualifier("nostroAccountReferentialMapperImpl") AccountReferentialMapper<NostroAccountReferential, NostroAccountReferentialDto> accountReferentialMapper,
        @Qualifier("nostroImportResultMapper") ReferentialImportResultMapper<NostroAccountReferential, NostroAccountReferentialDto> referentialImportResultMapper) {
        return new ReferentialImportService<>(referentialService, accountReferentialMapper, referentialImportResultMapper);
    }
}
