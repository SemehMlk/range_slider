@Configuration
public class ReferentialImportConfiguration {

    @Bean("migratedAccountReferentialMapperImpl")
    public AccountReferentialMapper<MigratedAccountReferential, MigratedAccountReferentialDto> migratedAccountReferentialMapper(MigratedAccountReferentialMapper mapper) {
        return mapper;
    }

    @Bean("nostroAccountReferentialMapperImpl")
    public AccountReferentialMapper<NostroAccountReferential, NostroAccountReferentialDto> nostroAccountReferentialMapper(NostroAccountReferentialMapper mapper) {
        return mapper;
    }

    @Bean("migratedImportService")
    public ReferentialImportService<MigratedAccountReferential, MigratedAccountReferentialDto> migratedImportService(
        ReferentialService<MigratedAccountReferential> referentialService,
        @Qualifier("migratedAccountReferentialMapperImpl") AccountReferentialMapper<MigratedAccountReferential, MigratedAccountReferentialDto> mapper,
        ReferentialImportResultMapper<MigratedAccountReferential, MigratedAccountReferentialDto> resultMapper,
        ReferentialFieldValidator<MigratedAccountReferential> validator) {
        return new ReferentialImportService<>(referentialService, mapper, resultMapper, validator);
    }

    @Bean("nostroImportService")
    public ReferentialImportService<NostroAccountReferential, NostroAccountReferentialDto> nostroImportService(
        ReferentialService<NostroAccountReferential> referentialService,
        @Qualifier("nostroAccountReferentialMapperImpl") AccountReferentialMapper<NostroAccountReferential, NostroAccountReferentialDto> mapper,
        ReferentialImportResultMapper<NostroAccountReferential, NostroAccountReferentialDto> resultMapper,
        ReferentialFieldValidator<NostroAccountReferential> validator) {
        return new ReferentialImportService<>(referentialService, mapper, resultMapper, validator);
    }
}
