   default void updateEntityFromDto(KemmStreamDto dto, @MappingTarget KemmStream entity) {
        if (entity instanceof KemmMqStream && dto instanceof KemmMqStreamDto) {
            updateMqStreamFromDto((KemmMqStreamDto) dto, (KemmMqStream) entity);
        } else {
            // This part must also handle the generic fields if it's not MQStream
            org.mapstruct.factory.Mappers.getMapper(StreamMapper.class).updateEntityFromDto(dto, entity);
        }
    }
