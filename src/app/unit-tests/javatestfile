components:
  schemas:
    KemmMqManagerDto:
      type: object
      properties:
        id:
          type: integer
          format: int64
        queueManager:
          type: string
        channel:
          type: string
        host:
          type: string
        mqUser:
          type: string
        port:
          type: integer
          format: int32
      required:
        - id
        - queueManager
        - channel
        - host
        - mqUser
        - port



paths:
  /mq-managers:
    get:
      summary: Récupérer tous les MQ Managers
      operationId: getAllMqManagers
      responses:
        '200':
          description: Liste des MQ Managers récupérée avec succès
          content:
            application/json:
              schema:
                type: array
                items:
                  $ref: '#/components/schemas/KemmMqManagerDto'
        '500':
          description: Erreur serveur interne
